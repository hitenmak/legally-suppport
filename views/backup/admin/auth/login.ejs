

<div class="login-box">
    <div class="login-logo">
        <img src="/fw2ezuyzb751/img/logo.png" alt="AdminLTE Logo" class="img-fluid m-auto " style="opacity: .8">
    </div>
    <div class="card">
        <div class="card-body login-card-body">
            <p class="login-box-msg">Sign in to start your session</p>

            <form action="/fw2ezuyzb751/login/check" method="POST">
                <div class="input-group mb-3">
                    <input type="email" name="email" id="email" class="form-control" placeholder="Email">
                    <div class="input-group-append">
                        <div class="input-group-text">
                            <span class="fas fa-envelope"></span>
                        </div>
                    </div>
                </div>
                <div class="input-group mb-3">
                    <input type="password" name="password" id="password" class="form-control" placeholder="Password">
                    <div class="input-group-append">
                        <div class="input-group-text">
                            <span class="fas fa-lock"></span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-12">
                        <button type="submit" class="btn bg-gradient-secondary btn-block loginBtn">Login</button>
                    </div>
                </div>
            </form>

            <!--<p class="mt-3 mb-1">
                <a href="login.html">Login</a>
            </p>-->
        </div>
    </div>
</div>

<script>
    $(document).ready(function () {
        $('#email').val('hello@mailinator.com');
        $('#password').val('hello');
        $('.loginBtn').trigger('click');
    })
</script>