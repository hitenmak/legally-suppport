<table width="100%" align="center" cellpadding="0" cellspacing="0"
    style="font-size:13px;color:#666666;border-collapse:collapse;border:1px solid #ccc;border-bottom:0">
    <tbody>
        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;padding:10px;width:66%">
                <b>Ticket Id: </b>
                <%= ticketId %>
            </td>
        </tr>
        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;padding:10px;width:66%">
                <b>User name: </b>
                <%= userName %>
            </td>
        </tr>
        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;padding:10px;width:66%">
                <b>Email: </b>
                <%= email %>
            </td>
        </tr>
        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;padding:10px;width:66%">
                <b>Category: </b>
                <%= category ? category : 'N/A' %>
            </td>
        </tr>
        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;padding:10px;width:66%">
                <b>Subcategory: </b>
                <%= subCategory ? subCategory : 'N/A' %>
            </td>
        </tr>

        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;padding:10px;width:66%">
                <b>Message: </b>
                <%= message %>
            </td>
        </tr>
        <tr>
            <td style="font-family:Arial,Helvetica,sans-serif;padding:1px;border-bottom:1px solid #ccc;width:66%">
                <a href="<%= detailsUrl%>" style="text-decoration: none">
                    <p class="cardOtp"
                        style="display: block; color:#000000;font-family: 'Inter', sans-serif;font-weight: 600;font-size: 14px;line-height: 1.2;padding: 10px;background: linear-gradient(267.06deg, #B68C03 3.32%, #FDEC70 95.76%);border-radius: 12px;margin: 0;text-align: center;">
                        Accept and Resolve
                    </p>
                </a>
            </td>
        </tr>

    </tbody>
</table>
<!-- Body -->
<!-- Card Round -->